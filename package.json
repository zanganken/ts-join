{
  "name": "@zanganken/ts-join",
  "version": "0.1.0",
  "description": "Global typing utility for ReadonlyArray.join to return the value of it as a type",
  "keywords": [
    "typescript",
    "typings",
    "types",
    "readonlyarray",
    "array",
    "join"
  ],
  "license": "MIT",
  "author": "Zanganken",
  "repository": "https://github.com/zanganken/ts-join",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    },
    "./array-join": {
      "types": "./dist/array-join.d.ts",
      "import": "./dist/array-join.mjs",
      "default": "./dist/array-join.js"
    }
  },
  "scripts": {
    "dev": "tsx src/",
    "build": "tsup && tsx scripts/build.ts"
  },
  "devDependencies": {
    "@types/node": "^22.13.14",
    "tsup": "^8.4.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2"
  }
}
